<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- Backgrounds -->
    <SolidColorBrush x:Key="MainBackground" Color="#FFEBE5"/>
    <SolidColorBrush x:Key="SecondaryBackground" Color="#FFE0B2"/>

    <!-- TopBar -->
    <SolidColorBrush x:Key="TopBarBackground" Color="#FF7043"/>
    <SolidColorBrush x:Key="TopBarBackground2" Color="#E64A19"/>
    <SolidColorBrush x:Key="TopBarForeground" Color="#FFF3E0"/>

    <!-- Buttons -->
    <SolidColorBrush x:Key="ButtonBackground" Color="#FFAB91"/>
    <SolidColorBrush x:Key="ButtonHoverBackground" Color="#FF7043"/>
    <SolidColorBrush x:Key="ButtonFocusedBackground" Color="#E64A19"/>
    <SolidColorBrush x:Key="ButtonForeground" Color="#FFF3E0"/>
    <SolidColorBrush x:Key="ButtonDisabledBackground" Color="#FFE0B2"/>
    <SolidColorBrush x:Key="ButtonDisabledForeground" Color="#FFB74D"/>

    <!-- Borders -->
    <SolidColorBrush x:Key="BorderBackground" Color="#FFCCBC"/>
    <SolidColorBrush x:Key="AccentBorder" Color="#FF7043"/>

    <!-- Text -->
    <SolidColorBrush x:Key="MainText" Color="#E65100"/>
    <SolidColorBrush x:Key="SecondaryText" Color="#BF360C"/>

    <!-- Highlights/Accents -->
    <SolidColorBrush x:Key="HighlightBackground" Color="#FFD54F"/>
    <SolidColorBrush x:Key="Accent" Color="#FF5722"/>

    <Color x:Key="MainBackgroundColor">#FFEBE5</Color>
    <Color x:Key="SecondaryBackgroundColor">#FFE0B2</Color>
    <Color x:Key="TopBarBackgroundColor">#FF7043</Color>
    <Color x:Key="TopBarBackgroundColor2">#E64A19</Color>
    <Color x:Key="ButtonBackgroundColor">#FFAB91</Color>
    <Color x:Key="ButtonHoverBackgroundColor">#FF7043</Color>
    <Color x:Key="ButtonFocusedBackgroundColor">#E64A19</Color>
    <Color x:Key="ButtonForegroundColor">#FFF3E0</Color>
    <Color x:Key="ButtonDisabledBackgroundColor">#FFE0B2</Color>
    <Color x:Key="ButtonDisabledForegroundColor">#FFB74D</Color>
    <Color x:Key="BorderBackgroundColor">#FFCCBC</Color>
    <Color x:Key="AccentBorderColor">#FF7043</Color>
    <Color x:Key="MainTextColor">#E65100</Color>
    <Color x:Key="SecondaryTextColor">#BF360C</Color>
    <Color x:Key="HighlightBackgroundColor">#FFD54F</Color>
    <Color x:Key="AccentColor">#FF5722</Color>


</ResourceDictionary>
